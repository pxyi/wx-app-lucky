.picker{
  width: 516rpx;
  height: 92rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 40rpx auto 0 auto;
  font-size: 32rpx;
}
.picker text:nth-child(1){
  font-size: 32rpx;
  color: #4A4A4A;
  height: 45rpx;
  line-height: 45rpx;
  margin: 23rpx 0;
  display: inline-block;
  border-right: 2rpx solid #ccc;
  padding:0 15px;
  vertical-align: middle;
}
.picker text:nth-child(2){
  display: inline-block;
  color: #888;
  padding: 0 20rpx;
  width: 224rpx;
  height: 45rpx;
  line-height: 45rpx;
  margin: 23rpx 0;
  vertical-align: middle;
}
