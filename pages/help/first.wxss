 height: 600rpx;
 overflow: hidden;
 position: relative;
}
.title view{
  text-align: center;
  position: absolute;
  width: 100%;
  height: 100%;
}
.title view:nth-child(1) image {
  width: 680rpx;
  height: 592rpx;
  margin-top: 86rpx;
}
.title view:nth-child(2) image{
  width: 540rpx;
  height: 130rpx;
  margin-top: 294rpx;
}
.title view:nth-child(3) button{
  width: 400rpx;
  height: 92rpx;
  background: #fe6028;
  font-size: 28rpx;
  color: #fff;
  border-radius: 46rpx;
  border: 10rpx solid #fcd032;
  margin-top: 460rpx;
}
.title view:nth-child(3) button.button-hover{
  background: #f77c50;
}



